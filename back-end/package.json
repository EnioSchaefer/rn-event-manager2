{
  "name": "back-end",
  "version": "1.0.0",
  "description": "Projeto Full-Stack em Node.js e React.js de uma aplicação de administração e inscrição de eventos. V2",
  "main": "./build/src/app.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node ./build/src/app.js",
    "db:restart": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:drop": "npx sequelize-cli db:drop",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
