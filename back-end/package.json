    "@types/node": "^20.10.1",
    "@types/qrcode": "^1.5.5",
    "@types/sequelize": "^4.28.19",
    "sequelize": "^6.35.1",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.3.2"
  },
